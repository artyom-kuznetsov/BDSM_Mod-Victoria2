	(Y  ) = { Left = "has_game_started" Right = Bool }
	(N Y) = { Left = "start_date" Right = Date } # Can have comparison operators

	(N Y) = { Left = "has_game_rule" Right = {
		Single = { Left = "name" Right = GameRuleKey }
		Single = { Left = "value" Right = { DicEnumValueFor = { Enum = GameRule KeySelector = "^\name\[R]" } } }
	} }
	(N Y) = { Left = "total_years_played" Right = { NnInt VariableName } } # Can have comparison operators
	(N N) = { Left = "log" Right = String }
	
	(N N) = { Left = { Type = Religion } Right = ReligionTrigger }
	(N N) = { Left = { Type = ReligionGroup } Right = ReligionTrigger }
	(N N) = { Left = { Type = Culture } Right = CultureTrigger }
	(N N) = { Left = { Type = CultureGroup } Right = CultureTrigger }
} }

(Type DistanceDef) = {
	Optional = { Left = "where" Right = { Province MaybeEventTargetProvTitle AnyTitle } }
	Optional = { Left = "value" Right = { NnDbl VariableName } }
	
	Optional = { Left = "who" Right = { OnlyCharTarget } }
	Optional = { Left = "distance" Right = { NnDbl VariableName } }
	
	#AllOrNone = { where value }
	#AllOrNone = { who distance }
	AtMostOne = { where who }
	AtMostOne = { distance value }
	OneOf = { where who }
	OneOf = { distance value }
}

(Type CheckConnectivityDef) = {
	Optional = { Left = "sea_gap" Right = Bool }
	Optional = { Left = "land_gap" Right = Bool }
	Optional = { Left = "sub_realm" Right = { Bool MaybeEventTargetChar AnyTitle CharTargetExcludingMaybeEventTarget } }
	Optional = { Left = "naval_distance" Right = { Bool NnInt } }
}

(Type IsConnectedToDef) = {
	Inherits = CheckConnectivityDef
	Single = { Left = "target" Right = { Literal = "realm_capital" Literal = "capital" Type = MaybeEventTargetProvTitle Type = Province Type = AnyTitle } }
}



(ContextValidator CharTrigger) = { Members = {
	(N N) = { Left = { "any_quester_targeting_this" "any_quester_targetting_this" } Right = CharTriggerWithCount }
	(Y  ) = { Left = "is_quest_target" Right = Bool }
	(Y  ) = { Left = "is_quest_target_of" Right = { MaybeEventTargetChar VariousOwners } }
} }

(ContextValidator CharCommand) = { Members = {
	(N N) = { Left = { "any_quester_targeting_this" "any_quester_targetting_this" } Right = CharMaybeLimitCommand }
	(Y  ) = { Left = "set_quest_target" Right = {
		# This used to have EventTargetSet
		# That was part of being used in scope "quest_target" which was marked as Untyped
		# I guess it was never implemented that way
		# If we do plan to implement it that way (which is the right way, but hard)
		# then we have to make sure that even if this setter is never used it is fine.
		# So probably include a checker somehow as well
		# Or have a separate quest thing or something
		Single = { Left = "id" Right = String (LocalOp "chk" "[R]" AddChecker CharQuest) }
		Optional = { Left = "society" Right = SocietyThings }
		Single = { Left = "holder" Right = OnlyCharTarget }
	} }
} }



