(Validator GlobalHistory) = {
	Type = Default
	FriendlyName = "Global history"
	(FileGroup "history\global" "*.txt")
	ValidatorName = GlobalHistory
}
	
(Type GlobalHistory) = {
	(Ch Y N) = { Left = { Type = Date } Right = DateClause }
}

(Type DateClause) = {
	Optional = { Left = "effect" Right = GlobalCommand }
}

