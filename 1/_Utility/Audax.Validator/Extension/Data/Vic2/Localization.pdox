	(DataSource Csv) = {
		(FileGroup "localisation" "*.csv")
		Columns = 5 # We don't really care about the rest and just want to be able to recognize as much as possible
		AllowExtraColumns = True
	}
	
	Key = ".\0\[R]"
	Value = ".\1\[R]"
